%p#notice= notice

%p
  - if current_admin
    %b Email:
    = @newsletter_subscriber.email
    %br
    %b Unsubscribe link:
    = @newsletter_subscriber.unsubscribe_path
    
- if current_admin    
  = link_to 'Edit', edit_newsletter_subscriber_path(@newsletter_subscriber)
  \|
  = link_to 'Back', newsletter_subscribers_path
